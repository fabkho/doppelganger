{
    "name": "your-vendor/laravel-data-sync",
    "description": "A flexible package for copying models and their relationships between different Laravel database connections",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\DataSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourVendor\\DataSync\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\DataSync\\DataSyncServiceProvider"
            ],
            "aliases": {
                "DataSync": "YourVendor\\DataSync\\Facades\\DataSync"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
